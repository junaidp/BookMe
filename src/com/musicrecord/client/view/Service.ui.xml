<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>

	<m:MaterialCard backgroundColor="WHITE">

		<m:MaterialCardImage waves="LIGHT">

			<m:MaterialImage  ui:field="image" />

			<m:MaterialCardTitle ui:field="imageTitle" />

		</m:MaterialCardImage>

		<m:MaterialCardContent textColor="BLACK">

			<m:MaterialLabel  ui:field="label"/>

		</m:MaterialCardContent>

		<m:MaterialCardAction>

			<m:MaterialLink ui:field="link1"/>
			<m:MaterialLink ui:field="link2"/>

		</m:MaterialCardAction>

	</m:MaterialCard>



</ui:UiBinder> 