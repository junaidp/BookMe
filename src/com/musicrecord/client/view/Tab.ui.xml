<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:v="urn:import:com.musicrecord.client.view">
	<g:HTMLPanel>
	<m:MaterialRow>
	<m:MaterialColumn grid="s12">
	<m:MaterialTab ui:field="tab" indicatorColor="YELLOW" shadow="1" grid = "s12">
	<m:MaterialTabItem waves="YELLOW" >
	<m:MaterialLink ui:field="tab1" text="My Bookings" href="#tab1" textColor="WHITE" waves="LIGHT">
	</m:MaterialLink>
	</m:MaterialTabItem>
	
	
	
	<m:MaterialTabItem waves="YELLOW">
	<m:MaterialLink text="SEARCH" iconType= "SEARCH" href="#tab2" textColor="WHITE" waves="LIGHT"></m:MaterialLink>
	</m:MaterialTabItem>
	</m:MaterialTab>
	</m:MaterialColumn>
	
	<m:MaterialColumn m:id="tab1" grid="s12" ui:field="tab1Container">
	
	</m:MaterialColumn>
	

	
	<m:MaterialColumn m:id="tab2" grid="s12">
	    <v:SearchAutoComplete></v:SearchAutoComplete>
	</m:MaterialColumn>

	</m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder> 